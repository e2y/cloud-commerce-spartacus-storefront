{
  "name": "storefrontapp",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "build": "ng build storefrontapp --prod",
    "build:core:lib": "ng build core && ng build storefrontlib",
    "build:qa": "ng build -c=qa",
    "e2e": "ng e2e",
    "e2e:ci": "ng e2e --configuration=ci --protractor-config=projects/storefrontapp-e2e/protractor.headless.conf.js",
    "lint": "ng lint",
    "mockserver": "cd ./projects/backend/mockgenerator && npm run start",
    "ng": "ng",
    "prettier": "prettier --config ./.prettierrc --list-different \"projects/**/*{.ts,.js,.json,.scss,.html}\"",
    "prettier-fix": "prettier --config ./.prettierrc --list-different \"projects/**/*{.ts,.js,.json,.scss,.html}\" --write",
    "start": "node server.js",
    "start:dev": "ng serve --proxy-config mock-server/proxy.backend.js",
    "start:mock": "ng serve --proxy-config mock-server/proxy.mock.js",
    "start:qa": "ng serve -c=qa",
    "start:prod": "ng serve --prod",
    "start:pwa": "cd ./dist/storefrontapp/ && http-server -p 4200",
    "test": "ng test",
    "test:core:lib": "concurrently \"ng test core --code-coverage\" \"ng test storefrontlib --code-coverage\"",
    "postinstall": "ng build core && ng build storefrontlib && ng build storefrontapp --prod"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^6.1.8",
    "@angular/common": "^6.1.8",
    "@angular/compiler": "^6.1.8",
    "@angular/core": "^6.1.8",
    "@angular/forms": "^6.1.8",
    "@angular/http": "^6.1.8",
    "@angular/platform-browser": "^6.1.8",
    "@angular/platform-browser-dynamic": "^6.1.8",
    "@angular/pwa": "^0.6.8",
    "@angular/router": "^6.1.8",
    "@angular/service-worker": "^6.0.0",
    "@ng-bootstrap/ng-bootstrap": "^3.2.2",
    "@ng-select/ng-select": "^2.9.1",
    "@ngrx/effects": "^6.1.0",
    "@ngrx/router-store": "^6.1.0",
    "@ngrx/store": "^6.1.0",
    "@spartacus/core": "^0.1.0-prealpha.3",
    "@spartacus/storefront": "^0.1.0-prealpha.8",
    "@spartacus/styles": "^0.1.0-prealpha.5",
    "@types/googlemaps": "^3.30.13",
    "bootstrap": "^4.1.3",
    "core-js": "^2.5.7",
    "crypto": "^1.0.1",
    "express": "^4.16.4",
    "hamburgers": "^0.9.3",
    "ngrx-store-localstorage": "^5.0.1",
    "path": "^0.12.7",
    "rxjs": "^6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.3",
    "@angular-devkit/build-ng-packagr": "~0.8.3",
    "@angular/cli": "~6.2.3",
    "@angular/compiler-cli": "^6.1.8",
    "@angular/language-service": "^6.1.8",
    "@ngrx/store-devtools": "^6.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.4.4",
    "concurrently": "^4.0.1",
    "faker": "^4.1.0",
    "http-server": "^0.11.1",
    "jasmine-core": "~2.99.1",
    "jasmine-marbles": "^0.3.1",
    "jasmine-spec-reporter": "~4.2.1",
    "json-server": "^0.12.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage": "^1.1.2",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^1.2.0",
    "ng-packagr": "^4.2.0",
    "node-sass": "^4.9.3",
    "prettier": "1.15.2",
    "protractor": "~5.4.1",
    "ts-node": "~5.0.1",
    "tsickle": "~0.32.1",
    "tslib": "^1.9.3",
    "tslint": "~5.11.0",
    "typescript": "2.9.2"
  },
  "browser" : {
    "crypto" : false
  }
}
